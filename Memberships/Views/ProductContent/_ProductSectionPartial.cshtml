@model IEnumerable<Memberships.Models.ProductSection>

@foreach (var sect in Model)
{
    <div class="panel panel-success">
        <div class="panel-heading">
            <h4 class="panel-title">
                <a data-toggle="collapse" class="panel-carret" href="#collapse@{@sect.SectionId}">
                    <span class="pull-left glyphicon glyphicon-play gly-rotate-90"></span>
                    @sect.Title
                </a>
            </h4>
        </div>
        <div id="collapse@{@sect.SectionId}" class="panel-collapse collapse in">
            <div class="panel-body">
                @foreach (var item in sect.ProductItemRows)
                {
                    @Html.Partial("_ProductItemRowPartial", item)
                }
            </div>
        </div>
    </div>
    <br />
}